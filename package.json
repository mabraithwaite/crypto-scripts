{
  "name": "crypto-scripts",
  "version": "1.0.0",
  "description": "Scripts used to monitor your crypto portfolio and the crypto market",
  "private": true,
  "scripts": {
    "clasp": "clasp",
    "clasp-create-spreadsheet": "rm -f .clasp.json && clasp create --title \"crypto-sheets\" --type sheets && clasp push && clasp open",
    "clasp-login": "clasp login",
    "clasp-logout": "clasp logout",
    "clasp-open": "clasp open",
    "clasp-push": "clasp push",
    "clasp-set-new-script-id": "rm -f .clasp.json && cp .clasp.json_bak .clasp.json && clasp setting scriptId"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mabraithwaite/crypto-scripts.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mabraithwaite/crypto-scripts/issues"
  },
  "homepage": "https://github.com/mabraithwaite/crypto-scripts#readme",
  "dependencies": {
    "@google/clasp": "^2.4.0"
  },
  "devDependencies": {
    "@types/google-apps-script": "^1.0.36"
  }
}
